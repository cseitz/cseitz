

server {
    listen 80;
    listen [::]:80;
    server_name dev.cseitz.dev;
    # return 302 https://$host$request_uri;
    proxy_intercept_errors on;
    include includes/repos/cseitz/errors;
    include includes/repos/cseitz/web-proxy;
}


server {
    listen 443 ssl;
    server_name dev.cseitz.dev;
    include includes/ssl/cseitz.dev;
    proxy_intercept_errors on;
    include includes/repos/cseitz/errors;
    # include includes/repos/cseitz/api-proxy;
    include includes/repos/cseitz/web-proxy;
}

