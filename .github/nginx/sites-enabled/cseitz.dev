

server {
    listen 80;
    listen [::]:80;
    server_name dev.cseitz.dev;
    # return 302 https://$host$request_uri;
    proxy_intercept_errors on;
    include includes/cseitz/errors;
    include includes/cseitz/web-proxy;
}


server {
    listen 443 ssl;
    server_name dev.cseitz.dev;
    ssl_certificate "/root/.acme.sh/cseitz.dev/cseitz.dev.fullchain.cer";
    ssl_certificate_key "/root/.acme.sh/cseitz.dev/cseitz.dev.key";
    proxy_intercept_errors on;
    include includes/cseitz/errors;
    # include includes/cseitz/api-proxy;
    include includes/cseitz/web-proxy;
}

